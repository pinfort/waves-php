{
    "name": "pinfort/waves-php",
    "description": "This is an api client for WAVES platform by PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "pinfort",
            "email": "ptg@nijitei.com"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "stephenhill/base58": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-BLAKE2": "*",
        "kornrunner/keccak": "^1.0",
        "ext-curve25519": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "cakephp/cakephp-codesniffer": "^3.1",
        "ext-fileinfo": "*",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Pinfort\\wavesPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pinfort\\wavesPHP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
